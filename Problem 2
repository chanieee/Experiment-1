#include<iostream>
#include<conio.h>
#include<math.h>
using namespace std;
int main()

{
	double mass, density, volume;
	cout<<"Enter Mass in Grams: "<<endl;
	cin>>mass;
	
	cout<<"Enter Density in Grams per Cubic Meter: "<<endl;
	cin>>density;
	
	volume=(mass/4)/density;

	cout<<"The volume is: "<<volume;

	getch();
	return 0;
}
